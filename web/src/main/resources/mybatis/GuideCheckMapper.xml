<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"

"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="GuideCheck">
	<resultMap type="GuideCheck"  id="guideCheck" >
		<id property="id" column="id" javaType="java.lang.String" />
		<result property="guideId" column="guide_id" javaType="java.lang.String" />
		<result property="result" column="result" javaType="java.lang.String" />
	</resultMap>

	<insert id="saveGuideCheck" parameterType="GuideCheck">
		insert into
		guide_check(id, guide_id, result) values (
		#{id}, #{guideId}, #{result})
	</insert>
	
</mapper>